{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "cth-blocks/cth-post-carousel",
  "version": "0.1.0",
  "title": "Posts Carousel",
  "category": "design",
  "icon": "leftright",
  "description": "Dynamic Posts Carousel",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "cth-post-carousel",
  "editorScript": "file:./index.js",
  "viewScript": "file:./swiper.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "blockID": {
      "type": "string",
      "default": "0"
    },
    "posts_per_page": {
      "type": "number",
      "default": 3
    },
    "post_type": {
      "type": "string",
      "default": "post"
    },
    "taxonomy": {
      "type": "string"
    },
    "terms": {
      "type": "array"
    },
    "order": {
      "type": "string",
      "default": "desc"
    },
    "orderby": {
      "type": "string",
      "default": "date"
    },
    "slides_per_view": {
      "type": "number",
      "default": 1
    },
    "navigation": {
      "type": "boolean",
      "default": true
    },
    "loop": {
      "type": "boolean",
      "default": false
    },
    "dots": {
      "type": "boolean",
      "default": false
    },
    "scrollbar": {
      "type": "boolean",
      "default": false
    }
  }
}